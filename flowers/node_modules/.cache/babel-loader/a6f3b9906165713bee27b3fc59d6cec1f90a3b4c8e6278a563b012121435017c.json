erItem\" v-for=\"item in pageData\" :key=\"item\">\r\n        <div>\r\n          <span class=\"x\">{{ item.username }}</span>\r\n          <span class=\"x\">{{ item.name }}</span>\r\n          <span class=\"x\">{{ item.gender }}</span>\r\n          <span class=\"x\">{{ item.age }}</span>\r\n          <span class=\"x\">{{ item.phone_num }}</span>\r\n          <span class=\"x\">{{ item.email }}</span>\r\n        </div>\r\n      </div> -->\r\n\r\n      <!-- 分页器 -->\r\n      <!-- <div style=\"margin-top: 20px\">\r\n        <el-pagination\r\n          v-model:current-page=\"findNewsPageInfo.pageNum\"\r\n          v-model:page-size=\"findNewsPageInfo.pageSize\"\r\n          @size-change=\"getPageList\"\r\n          @current-change=\"getPageList\"\r\n          :page-sizes=\"[5, 7, 10]\"\r\n          background\r\n          layout=\"prev, pager, next , ->, sizes, total\"\r\n          :total=\"totalSize\"\r\n        />\r\n      </div> -->\r\n            <div>\r\n                <!--      弹窗-->\r\n                <el-dialog v-model=\"dialogVisible\" title=\"操作\" width=\"30%\" @close=\"cancel\">\r\n\r\n                    <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"120px\">\r\n\r\n                        <el-form-item label=\"花名\" prop=\"f_name\">\r\n                            <el-input v-model=\"form.f_name\" disabled style=\"width: 80%\"></el-input>\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"颜色\" prop=\"f_color\">\r\n                            <el-input v-model=\"form.f_color\" disabled style=\"width: 80%\"></el-input>\r\n                        </el-form-item>\r\n\r\n                        <el-form-item  label=\"花语介绍\" prop=\"f_meanging\">\r\n                            <el-input v-model=\"form.f_meaning\" show-password style=\"width: 80%\"></el-input>\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"花期介绍\" prop=\"f_exist_time\">\r\n                            <el-input v-model=\"form.f_exist_time\" style=\"width: 80%\"></el-input>\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"产地\" prop=\"f_product_area\">\r\n                           <el-input v-model=\"form.f_product_area\" style=\"width: 80%\"></el-input>\r\n                            \r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"味道介绍\" prop=\"f_aroma\">\r\n                            <el-input v-model.number=\"form.f_aroma\" style=\"width: 80%\"></el-input>\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"花瓣介绍\" prop=\"f_petal_num\">\r\n                            <el-input v-model.number=\"form.f_petal_num\" style=\"width: 80%\"></el-input>\r\n                        </el-form-item>\r\n\r\n                        <el-form-item label=\"高度介绍\" prop=\"f_floower_hign\">\r\n                            <el-input v-model=\"form.f_floower_hign\" style=\"width: 80%\"></el-input>\r\n                        </el-form-item>\r\n\r\n                    </el-form>\r\n                    <template #footer>\r\n            <span class=\"dialog-footer\">\r\n              <el-button @click=\"cancel\">取 消</el-button>\r\n              <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n            </span>\r\n                    </template>\r\n                </el-dialog>\r\n            </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script >\r\nimport request from \"@/utils/index\";\r\n\r\nconst {ElMessage} = require(\"element-plus\");\r\nexport default {\r\n    name: \"FloowersInfo\",\r\n    data() {\r\n        // 手机号验证\r\n        return {\r\n            showpassword: true,\r\n            image: \"\",\r\n            editJudge: true,\r\n            disabled: true,\r\n            dialogVisible: false,\r\n            identity: \"\",\r\n            f_name: \"\",\r\n            f_color: \"\",\r\n            f_meaning: \"\",\r\n            f_exist_time: \"\",\r\n            f_product_area: \"\",\r\n            f_aroma: \"\",\r\n            f_petal_num:0,\r\n            f_floower_hign:0,\r\n            //targetURL: \"\",\r\n            //avatar: \"\",\r\n            form: {\r\n                f_name: \"\",\r\n                f_color: \"\",\r\n                f_meaning: \"\",\r\n                f_exist_time: \"\",\r\n                f_product_area: \"\",\r\n                f_aroma: \"\",\r\n                f_petal_num:0,\r\n                f_floower_hign:0,\r\n                f_create_date:\"\"\r\n            },\r\n            rules: {\r\n                name: [\r\n                    {required: true, message: \"请输入姓名\", trigger: \"blur\"},\r\n                    {\r\n                        pattern: /^(?:[\\u4E00-\\u9FA5·]{2,10})$/,\r\n                        message: \"必须由 2 到 10 个汉字组成\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n                gender: [{required: true, message: \"请选择性别\", trigger: \"change\"}],\r\n                age: [\r\n                    {required: true, message: \"请输入年龄\", trigger: \"blur\"},\r\n                    {type: \"number\", message: \"年龄必须为数字值\", trigger: \"blur\"},\r\n                    {\r\n                        pattern: /^(1|[1-9]\\d?|100)$/,\r\n                        message: \"范围：1-100\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n               \r\n                email: [\r\n                    {type: \"email\", message: \"请输入正确的邮箱地址\", trigger: \"blur\"},\r\n                ],\r\n                password: [\r\n                    {required: true, message: \"请输入密码\", trigger: \"blur\"},\r\n                    {\r\n                        min: 6,\r\n                        max: 32,\r\n                        message: \"长度在 6 到 16 个字符\",\r\n                        trigger: \"blur\",\r\n                    },\r\n                ],\r\n                \r\n            },\r\n            display: {\r\n                display: \"none\",\r\n            },\r\n            imgDisplay: {\r\n                display: \"none\",\r\n            },\r\n        };\r\n    },\r\n    created() {\r\n        this.find();\r\n        \r\n    },\r\n    methods: {\r\n        //查询数据，\r\n        find() {\r\n            this.form = JSON.parse(sessionStorage.getItem(\"user\"));\r\n            request.post(\"/\" + this.identity + \"/login\", this.form).then((res) => {\r\n                //更新Form\r\n                \r\n                //更新页面数据\r\n                this.load();\r\n            });\r\n        },\r\n        Edit() {\r\n            this.dialogVisible = true;\r\n            this.$nextTick(() => {\r\n                this.$refs.form.resetFields();\r\n                this.form = JSON.parse(sessionStorage.getItem(\"user\"));\r\n            });\r\n        },\r\n        cancel() {\r\n            this.$refs.form.resetFields();\r\n            this.display = {display: \"none\"};\r\n            this.showpassword = true;\r\n            this.editJudge = true;\r\n            this.disabled = true;\r\n            this.dialogVisible = false;\r\n        },\r\n        async save() {\r\n            this.$refs.form.validate(async (valid) => {\r\n                if (valid) {\r\n                    //修改\r\n                    await request.put(\"/\" + this.identity + \"/update\", this.form).then((res) => {\r\n                        if (res.code === 200) {\r\n                            ElMessage({\r\n                                message: \"修改成功\",\r\n                                type: \"success\",\r\n                            });\r\n                            //更新sessionStorage\r\n                            window.sessionStorage.setItem(\r\n                                \"user\",\r\n                                JSON.stringify(this.form)\r\n                            );\r\n                            this.find();\r\n                            this.dialogVisible = false;\r\n                        } else {\r\n                            ElMessage({\r\n                                message: res.msg,\r\n                                type: \"error\",\r\n                            });\r\n                        }\r\n                    });\r\n                }\r\n            });\r\n        },\r\n        \r\n        \r\n    },\r\n};\r\n</script>\r\n\r\n"],"mappings":"AAuIA,OAAOA,OAAM,MAAO,eAAe;AAEnC,MAAM;EAACC;AAAS,IAAIC,OAAO,CAAC,cAAc,CAAC;AAC3C,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACH;IACA,OAAO;MACHC,YAAY,EAAE,IAAI;MAClBC,KAAK,EAAE,EAAE;MACTC,SAAS,EAAE,IAAI;MACfC,QAAQ,EAAE,IAAI;MACdC,aAAa,EAAE,KAAK;MACpBC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE,EAAE;MACVC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE,EAAE;MAClBC,OAAO,EAAE,EAAE;MACXC,WAAW,EAAC,CAAC;MACbC,cAAc,EAAC,CAAC;MAChB;MACA;MACAC,IAAI,EAAE;QACFR,MAAM,EAAE,EAAE;QACVC,OAAO,EAAE,EAAE;QACXC,SAAS,EAAE,EAAE;QACbC,YAAY,EAAE,EAAE;QAChBC,cAAc,EAAE,EAAE;QAClBC,OAAO,EAAE,EAAE;QACXC,WAAW,EAAC,CAAC;QACbC,cAAc,EAAC,CAAC;QAChBE,aAAa,EAAC;MAClB,CAAC;MACDC,KAAK,EAAE;QACHlB,IAAI,EAAE,CACF;UAACmB,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAM,CAAC,EACnD;UACIC,OAAO,EAAE,8BAA8B;UACvCF,OAAO,EAAE,kBAAkB;UAC3BC,OAAO,EAAE;QACb,CAAC,CACJ;QACDE,MAAM,EAAE,CAAC;UAACJ,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAQ,CAAC,CAAC;QAC/DG,GAAG,EAAE,CACD;UAACL,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAM,CAAC,EACnD;UAACI,IAAI,EAAE,QAAQ;UAAEL,OAAO,EAAE,UAAU;UAAEC,OAAO,EAAE;QAAM,CAAC,EACtD;UACIC,OAAO,EAAE,oBAAoB;UAC7BF,OAAO,EAAE,UAAU;UACnBC,OAAO,EAAE;QACb,CAAC,CACJ;QAEDK,KAAK,EAAE,CACH;UAACD,IAAI,EAAE,OAAO;UAAEL,OAAO,EAAE,YAAY;UAAEC,OAAO,EAAE;QAAM,CAAC,CAC1D;QACDM,QAAQ,EAAE,CACN;UAACR,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAM,CAAC,EACnD;UACIO,GAAG,EAAE,CAAC;UACNC,GAAG,EAAE,EAAE;UACPT,OAAO,EAAE,gBAAgB;UACzBC,OAAO,EAAE;QACb,CAAC;MAGT,CAAC;MACDS,OAAO,EAAE;QACLA,OAAO,EAAE;MACb,CAAC;MACDC,UAAU