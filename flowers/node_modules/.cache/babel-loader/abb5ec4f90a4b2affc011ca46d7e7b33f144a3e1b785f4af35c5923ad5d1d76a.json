Manage() {\n      this.$router.push(\"/selfInfo\")\n    },\n    signup(){\n    console.log(this.form)\n        request.post(\"/\" + this.identify +\"/signup\",this.form).then(res => {\n                if (res.code === 200) {\n                            console.log(\"chengg\")\n                            ElMessage({\n                                message: \"签到成功\",\n                                type: \"success\",\n                            });\n                            window.sessionStorage.setItem(\"signUpStatus\", JSON.stringify(res.data));\n                           //签到成功刷新页面\n                             this.$router.push(\"/home\")\n                        } else {\n                            console.log(\"fail\")\n                            console.log(res.message)\n                            ElMessage({\n                                \n                                message: res.message,\n                                type: \"error\",\n                            });\n                        }\n        });\n       \n    }\n  },\n}\n</script>\n\n<style scoped>\n.right-info {\n  width: 120px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  margin-right: 1.5%;\n}\n\n.right-info:hover {\n  cursor: pointer;\n}\n</style>"],"mappings":";AA0BA,OAAOA,OAAM,MAAO,eAAe;AACnC,OAAOC,KAAI,MAAO,oBAAoB;AAEtC,MAAM;EAACC;AAAS,IAAIC,OAAO,CAAC,cAAc,CAAC;AAC3C,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLF,IAAI,EAAE,EAAE;MACRG,QAAQ,EAAC,EAAE;MAEXC,IAAI,EAAC;QACHC,QAAQ,EAAC;MAEX;IAEF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACH,IAAI,CAACC,IAAI,CAAC,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACND,IAAIA,CAAA,EAAG;MACA,IAAIL,IAAG,GAAIO,IAAI,CAACC,KAAK,CAACC,cAAc,CAACC,OAAO,CAAC,MAAM,CAAC,CAAC;MACrD,IAAI,CAACR,IAAI,CAACC,QAAO,GAAIH,IAAI,CAACG,QAAO;MACjC,IAAI,CAACF,QAAO,GAAKM,IAAI,CAACC,KAAK,CAACC,cAAc,CAACC,OAAO,CAAC,UAAU,CAAC,CAAC;IACrE,CAAC;IACHC,OAAOA,CAAA,EAAG;MACRF,cAAc,CAACG,KAAK,CAAC;MACrBlB,OAAO,CAACmB